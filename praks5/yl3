#!/bin/bash
#Tagastab tervituse saadud aja kohta
echo -n "Sisesta aja v채채rtus:"
read aeg
#Kui aeg on 6 ja 12 vahel
if [ $aeg -ge 6 -a $aeg -lt 12 ]
then
	echo "Tere hommikust!"
#kui aeg on 12 ja 18 vahel
elif [ $aeg -ge 12 -a $aeg -lt 18 ]
then
	echo "Tere paevast!"
#kui aeg on 18 ja 22 vahel
elif [ $aeg -ge 18 -a $aeg -lt 22 ]
then
	echo "Tere ohtust!"
#kui aeg on 22 ja 24 vahel v천i 0 ja 6 vahel
elif [ $aeg -ge 22 -a $aeg -lt 24 ] || [ $aeg -ge 0 -a $aeg -lt 6 ]
then
	echo "Head ood!"
#Kui sisestatud arv ei ei ole 0 ja 24 vahel (24 v채lja arvatud)
else
	echo "Vale sisend"
fi
